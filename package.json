{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:ci": "eslint --ext .js,.vue --ignore-path .gitignore",
    "lint:js": "eslint --ext '.js,.vue' --ignore-path .gitignore",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.{js,vue}'",
    "lintfix": "npm run lint:js -- --fix && npm run lint:prettier -- --write",
    "pre-commit": "npx pretty-quick --staged && npm run lintfix",
    "test": "echo 'Coming soon, hopefully!'"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.0",
    "husky": "^9.0.11",
    "nuxt": "^3.10.2",
    "nuxt-icon": "^0.6.8",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "pretty-quick": "^4.0.0",
    "sass": "^1.71.0"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.1",
    "@pinia/nuxt": "^0.5.1",
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "tailwindcss": "^3.4.1",
    "underscore": "^1.13.6",
    "vue": "^3.4.19",
    "vue-gtag-next": "^1.14.0",
    "vue-showdown": "^4.2.0"
  }
}
